{
  "name": "seimur",
  "version": "0.0.1",
  "description": "al framework",
  "author": "ah",
  "private": true,
  "scripts": {
    "webpack-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --progress --colors --config ./config/webpack_settings/dev.js --host 0.0.0.0 --watch-poll 3000",
    "web-server": "node ./node_modules/http-server/bin/http-server -p 3000 .",
    "start": "npm run webpack-server & npm run web-server",
    "build-prod": "node ./node_modules/webpack/bin/webpack.js --progress --colors --config ./config/webpack_settings/prod.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --progress --colors --config ./webpack.config.js"
  },
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "source-map-loader": "0.1.5",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "lodash": "4.14.0",
    "querystring": "0.2.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  }
}
